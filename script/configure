#! /bin/sh

rm -rf .env.dev
touch .env.dev

cat <<EOL >.mongo.dev.env
MONGO_INITDB_ROOT_USERNAME=
MONGO_INITDB_ROOT_PASSWORD=
MONGO_INITDB_DATABASE=
MONGO_INITDB_USERNAME=
MONGO_INITDB_PASSWORD=
EOL

cat <<EOL >.service.dev.env
PORT=50051

MONGO_URL=
MONGO_DATANAME=
MONGO_COLLECTION_NAME=

API_SECRET=
API_KEY=
EOL
